<%= form_for(resource, as: resource_name, url: new_user_session_path) do |f| %>
  <%= render partial: "/layouts/header", locals: {name: "LOG IN", f: f} %>

  <div class="field">
    <%= f.email_field :email, autocomplete: "email", placeholder: "Email", class: "form" %>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password", class: "form" %>
  </div>
<% end %>
